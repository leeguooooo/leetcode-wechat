{
  "name": "zhihu-lint",
  "version": "4.7.0",
  "description": "Donâ€™t think, just write!",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://git.in.zhihu.com/fe/zhihu-lint/issues"
  },
  "homepage": "https://git.in.zhihu.com/fe/zhihu-lint",
  "bin": "./bin/zhihu-lint.js",
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "lint": "node ./bin/zhihu-lint.js",
    "lint:fix": "node ./bin/zhihu-lint.js --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --notify",
    "prepare": "npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "files": [
    "bin",
    "lib",
    ".eslintrc.js",
    ".eslintrc.dev.js",
    ".prettierrc.js",
    "prettier.config.js"
  ],
  "jest": {
    "coverageReporters": [
      "text-summary",
      "html",
      "cobertura"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@git.in.zhihu.com:fe/zhihu-lint.git"
  },
  "author": "zhuoran",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "debug": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^1.0.0",
    "find-cache-dir": "^3.0.0",
    "globby": "^8.0.1",
    "ignore": "^3.3.5",
    "is-ci": "^2.0.0",
    "lodash": "^4.17.4",
    "npm-which": "^3.0.1"
  },
  "devDependencies": {
    "@zhihu/auto-publish": "^3.5.3",
    "husky": "^1.3.1",
    "jest": "^23.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^2.1.2",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "prettier": "^1.17.0 || ^2"
  }
}
